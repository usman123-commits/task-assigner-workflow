    {
      "parameters": {
        "jsCode": "const storedTimestamp = $('Accumulate Leads').last().json.storedTimestamp || '1970-01-01T00:00:00.000Z';\nconst items = $input.all();\nconst filtered = [];\nfor (const item of items) {\n  const lead = item.json;\n  const created = lead?.metadata?.createdUtcDateTime;\n  if (created && created > storedTimestamp) {\n    filtered.push(item);\n  }\n}\nreturn filtered;"
      },
      "id": "f9926d0b-309d-4ea1-9cbd-8490ec388c67",
      "name": "Filter New Bookings",
